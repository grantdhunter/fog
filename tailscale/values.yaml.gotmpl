operatorConfig:
  extraEnv:
    - name: PROXY_PRIORITY_CLASS_NAME
      value: critical

resources:
  requests:
    cpu: 10m
    memory: 48Mi
  limits:
    cpu: 500m
    memory: 64Mi

oauth:
  clientId: {{ requiredEnv "TAILSCALE_OAUTH_CLIENT_ID" }}
  clientSecret: {{ requiredEnv "TAILSCALE_OAUTH_CLIENT_SECRET" }} 